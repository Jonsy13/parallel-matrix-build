type Project {
  id: ID!
  name: String!
  members: [Member!]!
  state: String
  created_at: String!
  updated_at: String!
  removed_at: String!
}

type Member {
  user_id: ID!
  user_name: String!
  name: String!
  email: String!
  role: MemberRole!
  invitation: String!
  joined_at: String!
  deactivated_at: String!
}

input MemberInput {
  project_id: ID!
  user_id: String!
  role: MemberRole
}

enum MemberRole {
  Owner
  Editor
  Viewer
}
